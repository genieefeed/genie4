{
  "version": 2,
  "builds": [
    {
      "src": "api/tiktok/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/tiktok",
      "dest": "/api/tiktok/authorize.js"
    },
    {
      "src": "/auth/tiktok/callback",
      "dest": "/api/tiktok/callback.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}